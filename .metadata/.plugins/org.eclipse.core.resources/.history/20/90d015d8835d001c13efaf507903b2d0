package kr.co.ezenac.pay.model.dao;

import org.apache.ibatis.session.SqlSession;
import org.springframework.beans.factory.annotation.Autowired;
import org.springframework.stereotype.Repository;

import kr.co.ezenac.pay.model.vo.PayVO;

@Repository("pDAO")
//implement와 연결할 아이디
public class PayDAO {

	@Autowired
	private SqlSession sqlSession;
	private static final String nameSpace="payMapper";
	
	public void insertPay(PayVO pvo) throws Exception{
		sqlSession.insert(nameSpace + ".insertPay", pvo);
	}
}
